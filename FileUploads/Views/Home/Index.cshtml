@model IEnumerable<FileUploads.Models.fileupload>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.filename)
        </th>
        <th>
            IMAGE
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.filename)
            </td>
            <td>
                <img src="@Url.Content(item.file_path)" height="100" width="100" />
            </td>
        </tr>
    }
</table>